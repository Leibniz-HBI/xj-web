{
  "name": "lokaler-xjournalism",
  "version": "1.0.1",
  "description": "lokaler-xjournalism React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "prettier": "prettier --single-quote --trailing-comma es5 --no-semi",
    "format": "npm run prettier -- --write '{src,demo/src}/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "deploy": "npm run build && gh-pages -d demo/dist",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "d3-dsv": "^1.1.1",
    "d3-scale": "^3.2.0",
    "d3-scale-chromatic": "^1.5.0",
    "dayjs": "^1.8.16"
  },
  "peerDependencies": {
    "react": "16.x",
    "@material-ui/core": "4.x",
    "@material-ui/icons": "4.x",
    "@material-ui/styles": "4.x"
  },
  "devDependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.5.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.16.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "lodash": "^4.17.15",
    "nwb": "0.23.x",
    "prettier": "^1.18.2",
    "raw-loader": "^3.1.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-live": "^2.2.1",
    "typeface-barlow": "0.0.71",
    "typeface-roboto": "0.0.75"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,demo/src}/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "npm run prettier -- --write",
      "git add"
    ]
  }
}
